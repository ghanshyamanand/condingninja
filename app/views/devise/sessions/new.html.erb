<div class="border-form-div" style="margin-top: 10% !important;">
  <h2>Agent Sign in</h2>
  <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
    <%= f.email_field :email, :autofocus => true, :placeholder => 'Email address', :class => "sign_in_textbox" %>
    <br>
    <%= f.password_field :password, :placeholder => 'Password', :class => "sign_in_textbox" %>
    <br>
    <%= f.submit "Sign in",:class=>'btn btn-primary' %>
  <% end %>
  <br>
  <%= render "devise/shared/links" %>
</div>
